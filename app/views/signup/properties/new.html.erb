<%= render 'signup/shared/signup_tabs', enabled_tab: 3 %>
<div class="row signup-form">
  <div class="col-6">
    <h3>Add a property</h3>  
    <%= render 'layouts/flash' %>      
    <%= render 'form' %>
  </div>
  <div class="col-6">
    <h3>Added Properties</h3>
    <%= render 'properties_table', :properties => @customer.properties %>
    <%= link_to 'Next >', signup_customer_path(@customer), role: 'button', class: 'btn btn-success' %>
  </div>
</div>
